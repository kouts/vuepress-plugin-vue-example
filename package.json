{
  "name": "vuepress-plugin-vue-example",
  "description": "A Vuepress 2 plugin to display Vue components' live examples and source code inside documentation",
  "version": "0.0.0-semantic-release",
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "repository": "https://github.com/kouts/vuepress-plugin-vue-example",
  "keywords": [
    "vuepress",
    "code",
    "example",
    "vue",
    "demo",
    "demo-code",
    "documentation"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vuepress dev docs",
    "lint": "eslint . --ext .js,.vue",
    "lint-fix": "eslint . --fix --ext .js,.vue",
    "prepare": "husky install"
  },
  "dependencies": {
    "raw-loader": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@kouts/eslint-config": "^0.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.5",
    "@vue/vue3-jest": "^27.0.0",
    "@vuepress/plugin-search": "^2.0.0-beta.53",
    "bootstrap": "^4.6.2",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "jest-raw-loader": "^1.0.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "prismjs": "^1.29.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vuepress-webpack": "^2.0.0-beta.53"
  }
}
