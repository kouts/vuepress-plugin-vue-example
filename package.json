{
  "name": "vuepress-plugin-vue-example",
  "description": "A Vuepress plugin to display Vue components' live examples and source code inside documentation",
  "version": "0.0.0-semantic-release",
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "repository": "https://github.com/kouts/vuepress-plugin-vue-example",
  "keywords": [
    "vuepress",
    "code",
    "example",
    "vue",
    "demo",
    "demo-code",
    "documentation"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vuepress dev docs",
    "lint": "eslint . --ext .js,.vue",
    "lint-fix": "eslint . --fix --ext .js,.vue",
    "prepare": "husky install"
  },
  "dependencies": {
    "raw-loader": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0",    
    "bootstrap": "^4.6.2",
    "eslint": "^8.20.0",
    "eslint-config-kouts": "^2.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jest-raw-loader": "^1.0.1",
    "prettier": "2.7.1",
    "prismjs": "^1.28.0",
    "sass": "^1.54.0",
    "sass-loader": "^10.3.1",
    "vue-template-compiler": "^2.7.8",
    "vuepress": "^1.9.7"
  }
}
